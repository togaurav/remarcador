<?xml version="1.0" encoding="UTF-8"?>
<web-app 
	version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	
	<display-name>REMARCADOR - PROJECT</display-name>
	<description>REMARCADOR - PROJECT</description>      
         
	<servlet>
		<servlet-name>remarcador</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
  		
	<servlet-mapping>
		<servlet-name>remarcador</servlet-name>
	    <url-pattern>*.htm</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
    	<welcome-file>index.jsp</welcome-file>
  	</welcome-file-list>

	<resource-ref>
		<description>PostgreSQL datasource WamManagerDS</description>
	 	<res-ref-name>jdbc/WamManagerDS</res-ref-name>
	 	<res-type>javax.sql.DataSource</res-type>
	 	<res-auth>Container</res-auth>
	</resource-ref>

	<resource-ref>
		<description>PostgreSQL datasource CencosudPulsosDS</description>
	 	<res-ref-name>jdbc/CencosudPulsosDS</res-ref-name>
	 	<res-type>javax.sql.DataSource</res-type>
	 	<res-auth>Container</res-auth>
	</resource-ref>
</web-app>